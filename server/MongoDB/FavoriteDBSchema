const mongoose = require('mongoose');
const Schema  = mongoose.Schema;

const FavCard = new Schema({
    cardname: {
        type: String,
        required: true,
    },
    image_url: {
        type: String,
        required: true,
    },
    isFav: {
        type: Boolean,
        required: true,
    },
})
module.exports=mongoose.model('FavoriteCard', FavCard);